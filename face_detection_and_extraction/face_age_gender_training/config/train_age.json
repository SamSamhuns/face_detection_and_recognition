{
    "name": "ResMLP_age",
    "n_gpu": 1,
    "amp": true,
    "checkpoint": null,
    "arch": {
        "type": "ResMLP",
        "args": {
            "dropout": 0.05,
            "num_residuals_per_block": 1,
            "num_blocks": 4,
            "num_classes": 5,
            "num_initial_features": 512,
            "reduce_in_features": null,
            "add_residual": true,
            "add_IC": true
        }
    },
    "data_loader": {
        "type": "AgeDataLoader",
        "args": {
            "data_dir": "../data/",
            "batch_size": 128,
            "shuffle": true,
            "validation_split": 0,
            "num_workers": 8,
            "dataset": "wiki",
            "dataset_test": null,
            "num_classes": 5
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 0.0026525028717740277,
            "weight_decay": 6.5025649191944946e-06
        }
    },
    "loss": "cse",
    "train_metrics": [
        "accuracy",
        "top_3_acc",
        "top_5_acc"
    ],
    "test_metrics": [
        "accuracy",
        "top_3_acc",
        "top_5_acc",
        "accuracy_per_class"
    ],
    "lr_scheduler": {
        "type": "ExponentialLR",
        "args": {
            "gamma": 0.2921212208238253
        }
    },
    "trainer": {
        "epochs": 5,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 5,
        "tensorboard": true
    }
}
